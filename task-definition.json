{
    "family": "fargate-ecs", 
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "flask-app", 
            "image": "731253471521.dkr.ecr.us-east-1.amazonaws.com/docker-ecs-repo:27", 
            "portMappings": [
                {
                    "containerPort": 5000, 
                    "hostPort": 5000, 
                    "protocol": "tcp"
                }
            ]
            
        }
    ], 
    "requiresCompatibilities": [
        "FARGATE"
    ], 
    "cpu": "256", 
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::731253471521:role/ecsTaskExecutionRole"
}